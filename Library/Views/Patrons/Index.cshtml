<h1>Library</h1>
<hr />
<h2>Patrons:</h2>

@if (Model.Count != 0)
{
  <ul>
    @foreach (var patron in Model)
    {
      <li><a href='/patrons/@patron.GetId()'>@patron.GetName()</a></li>
      <li><a href='/patrons/@patron.GetId()'>@patron.GetPhoneNumber()</a></li>
    }
  </ul>
}

<h4><a href="/patrons/new">Add new patron</a></h4>
<h4><a href="/">Return home</a></h4>
